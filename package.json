{
  "name": "fork-transporter",
  "version": "1.0.0",
  "description": "Fork ipc communicator using rxjs",
  "main": "dist/index.js",
  "author": "Alex Aldana",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage:html": "nyc report --reporter=html",
    "lint": "tslint -c tslint.json -t stylish -p .",
    "lint:fix": "tslint -c tslint.json -p . --fix",
    "test": "mocha"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist/**/*.*",
      "gulpfile.js",
      "coverage"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.4",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "rxjs": "^6.2.2"
  }
}
